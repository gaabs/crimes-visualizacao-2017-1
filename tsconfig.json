{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false
  },
  "exclude": [
    "node_modules"
  ],
  "typeRoots": [
    "node_modules/@types"
  ]
}